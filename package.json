{
  "name": "speed-dial",
  "version": "1.0.5",
  "description": "You can set a button in every corner of the page and display smaller buttons in each direction.",
  "main": "docs/speedDial.min.js",
  "author": "Grzegorz Tomicki",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"yarn sass:prod\" \"yarn sass:dev\" \"yarn watch\"",
    "prod": "yarn build && yarn sass:prod && yarn postcss",
    "watch": "rollup -c -w",
    "build": "rollup -c --environment PRODUCTION",
    "sass:dev": "node-sass -w -r sources/scss/style.scss docs/speedDial.min.css",
    "sass:prod": "node-sass sources/scss/style.scss docs/speedDial.min.css --output-style compressed",
    "postcss": "postcss docs/speedDial.min.css -o docs/speedDial.min.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomik23/speed-dial-menu.git"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "autoprefixer": "^10.0.1",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "cssnano": "^4.1.10",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "node-sass": "^4.14.1",
    "postcss-cli": "^8.1.0",
    "postcss-css-variables": "^0.17.0",
    "prettier": "^2.1.2",
    "rollup": "^2.32.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}