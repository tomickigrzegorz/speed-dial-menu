{
  "name": "speed-dial",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/tomik23/speed-dial.git",
  "license": "MIT",
  "scripts": {
    "dev": "node-sass style.scss style.css --watch",
    "prod": "node-sass style.scss style.css --output-style compressed",
    "postcss": "postcss style.css -o style.css",
    "build": "concurrently \"yarn prod\" \"yarn postcss\""
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-css-variables": {},
      "cssnano": {}
    }
  },
  "browserslist": [
    "last 2 version",
    ">1%",
    "not dead"
  ],
  "devDependencies": {
    "autoprefixer": "^9.7.2",
    "concurrently": "^5.0.0",
    "cssnano": "^4.1.10",
    "node-sass": "^4.13.0",
    "postcss-cli": "^6.1.3",
    "postcss-css-variables": "^0.14.0"
  }
}